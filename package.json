    "@stripe/react-stripe-js": "^2.8.1",
    "@stripe/stripe-js": "^4.9.0",
    "@types/js-yaml": "^4.0.9",
    "dotenv": "^16.4.1",
    "framer-motion": "^11.11.10",
    "js-yaml": "^4.1.0",
    "openai": "^4.69.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0"
